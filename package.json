{
  "name": "azul-tiles",
  "version": "1.0.0",
  "description": "Implements the board game Azul",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
"type": "module",
  "scripts": {
    "test": "jest",
"build": "tsc",
"buildExamples": "rm -rf examples/*js && npx tsc -p examples/",
"buildBenchmarks": "rm -rf benchmarks/*js && npx tsc -p benchmarks/",
"docs": "typedoc",
"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
"lint": "eslint .",
"prepare": "npm run build",
"prepublishOnly": "exit 0",
"preversion": "npm run lint",
"version": "npm run format && git add -A src",
"postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domw95/azul-tiles.git"
  },
  "keywords": [
    "azul",
    "game",
    "ai"
  ],
  "devDependencies": {
        "@types/jest": "^29.2.5",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "benny": "^3.7.1",
        "eslint": "^8.31.0",
        "jest": "^29.3.1",
        "prettier": "^2.8.2",
        "ts-jest": "^29.0.3",
        "typedoc": "^0.23.24",
        "typescript": "^4.9.4"
    },
  "author": "domw95",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/domw95/azul-tiles/issues"
  },
  "homepage": "https://github.com/domw95/azul-tiles#readme"
}
